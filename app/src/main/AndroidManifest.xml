<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crud_v6">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crud_v6">
        <activity
            android:name=".VerActivity"
            android:label="Contacto"
            android:parentActivityName=".MenuActivity"
            android:exported="true" />

        <activity
            android:name=".MenuActivity"
            android:parentActivityName=".MainActivity"
            android:label="Mi Agenda"
            android:exported="true" />
        <activity
            android:name=".EditarActivity"
            android:label="Editar Registro"
            android:parentActivityName=".MenuActivity"
            android:exported="true" />
        <activity
            android:name=".NuevoActivity"
            android:label="Nuevo Registro"
            android:exported="true"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Proyecto Agenda"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>